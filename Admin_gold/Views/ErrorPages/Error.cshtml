@{
    ViewData["Title"] = "Error";
    ViewData["messsage"] = Model;
    string data = ViewData["messsage"].ToString();
    data = data.Replace("{", "").Replace("}", "");
}

<div id="preloader">
    <div class="container h-100">
        <div class="row align-items-center h-100">
            <div class="col-6 mx-auto">
                <div class="jumbotron" style="background-color: #fff">
                    <div class="card text-center">
                        <div class="card-header">

                        </div>
                        <div class="card-body">
                            <center><img style="width: 250px;" src="~/images/logo_fibremex.png" alt="Alternate Text" /></center>
                            <p class="card-text mt-5">@data.Replace("id = ", "")</p>
                            <a href="#" onclick="history.go(-1);" class="btn btn-sm btn-primary">
                                Regresar
                            </a>
                        </div>
                        <div class="card-footer text-muted">

                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>